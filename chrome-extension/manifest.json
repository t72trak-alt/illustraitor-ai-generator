{
  "manifest_version": 3,
  "name": "Illustraitor AI",
  "version": "2.0.0",
  "description": "Генератор изображений",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:8000/*",
    "https://api.openai.com/*",
    "https://api.unsplash.com/*"
  ]
}